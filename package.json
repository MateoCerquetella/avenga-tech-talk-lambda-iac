  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.8",
    "serverless": "^4.4.12",
    "@aws-sdk/client-s3": "^3.310.0",
    "@aws-sdk/client-rekognition": "^3.310.0"
  }
}